{
  "name": "@beavercoding/uni-typer",
  "version": "0.0.8",
  "description": "Uni typer is a library that helps you to display the type effect of a unicode string on your website.",
  "types": "dist/index.d.mts",
  "source": "src/index.mts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "type": "module",
  "scripts": {
    "start": "node ./dist/index.mjs",
    "build": "rm -rf ./dist && rollup -c",
    "watch": "rollup -cw",
    "test": "vitest",
    "release": "release-it"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beavercoding2022/uni-splitter.git"
  },
  "keywords": [
    "unicode",
    "splitter",
    "unicode-splitter",
    "unicode-components",
    "unicode-decomposition"
  ],
  "author": "Orlein <pcj9024@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-react": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@release-it/conventional-changelog": "^8.0.1",
    "@rollup/plugin-alias": "^5.1.0",
    "@rollup/plugin-swc": "^0.3.1",
    "@rollup/plugin-typescript": "^11.1.6",
    "@swc-node/core": "^1.13.1",
    "@swc/cli": "^0.4.0",
    "@swc/core": "^1.6.6",
    "conventional-changelog-eslint": "^6.0.0",
    "release-it": "^17.4.1",
    "rollup": "^4.18.0",
    "rollup-plugin-dts": "^6.1.1",
    "tslib": "^2.6.3",
    "typescript": "^5.5.2",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.6.0"
  },
  "bugs": {
    "url": "https://github.com/beavercoding2022/uni-typer/issues"
  },
  "homepage": "https://github.com/beavercoding2022/uni-typer#readme",
  "packageManager": "pnpm@9.0.6",
  "directories": {
    "test": "test"
  }
}
